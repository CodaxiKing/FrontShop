{
  "expo": {
    "name": "AppVendas",
    "slug": "criare-sistemas",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.criaredev.io.criaresistemas",
      "versionCode": 5,
      "jsEngine": "hermes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-camera",
        {
          "cameraPermission": "$(PRODUCT_NAME) necessita do acesso à câmera",
          "microphonePermission": "$(PRODUCT_NAME) necessita do acesso ao microfone",
          "recordAudioAndroid": true
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d1327ac-4de1-4b8f-8641-bdd8cc7c9cb3"
      }
    }
  }
}
